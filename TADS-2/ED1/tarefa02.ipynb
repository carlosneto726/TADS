      "source": [
        "def media(n):\n",
        "  if n == 0:\n",
        "    return 1\n",
        "  else:\n",
        "      return (n + (( 4 + ( n ** 2)) ** 0.5 )) / 2\n",
        "\n",
        "n = int(input(\"Digite um número para uma média metálica: \"))\n",
        "print(f\"O valor da média metálica de {n} é {media(n)}\")\n"
      ],
